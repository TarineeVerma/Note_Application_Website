<div class="searchcontainer">

    <div class="sharchrow">
      <% if(searchResults != '') { %>
        <% searchResults.forEach(function (note, index) { %>
          
          <a href="/dashboard/item/<%= note._id %>">
          <h4><%= note.title %></h4>
          </a>
  
        <% }) %>
      <% } else { %>
        <p>No items found.</p>
      <% } %>
  
    </div>
  
  </div>